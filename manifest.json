{
    "manifest_version": 2,

    "name": "PSTui",
    "description": "This extension serves to alter the experience on playstationtrophies.org.",
    "version": "1.44",

    "browser_action": {
      "default_icon": "icon.png"
    },

    "permissions": [
        "tabs",
        "activeTab",
        "storage"
    ],

    "content_scripts": [
        {
            "matches": ["http://www.playstationtrophies.org/forum/*"],
            "css": ["main.css"],
            "js": ["jquery-2.1.3.min.js", "javascript.js", "options.js"]
        }
    ]
}